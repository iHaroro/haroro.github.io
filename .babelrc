{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "import",
      {
        "libraryName": "zarm",
        "style": true
      },
      "zarm"
    ],
    [
      "import",
      {
        "libraryName": "ahooks",
        "camel2DashComponentName": false,
        "camel2UnderlineComponentName": false
      },
      "ahooks"
    ]
  ]
}
